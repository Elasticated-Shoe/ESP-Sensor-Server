<!doctype html>
<html class="no-js" lang="en" dir="ltr">
    <head>
        {{ include('partial/head.twig') }}
    </head>
    <body>
        <div class="grid-y body-grid">
            <div id="header-container" class="cell shrink">
                {{ include('partial/header.twig') }}
            </div>  
            <div id="content-container" class="cell shrink">
                {% block content %}
                    There is no content available for this page yet
                {% endblock %}
            </div>
            <div class="footer-container cell auto">
                {{ include('partial/footer.twig') }}
            </div>
        </div>
        <script src="assets/js/vendor/jquery.js"></script>
        <script src="assets/js/vendor/what-input.js"></script>
        <script src="assets/js/vendor/slick.min.js"></script>
        <script src="assets/js/vendor/underscore.js"></script>
        <script src="assets/js/vendor/fuse.js"></script>
        <script src="assets/js/vendor/foundation.min.js"></script>
        <script src="assets/js/app.js"></script>
        {% for script in scripts %}
            <script src="{{ script }}"></script>
        {% endfor %}
        {% for template in templates %}
            {{ include(template) }}
        {% endfor %}
    </body>
</html>